<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="demo">
	<insert id="insertDemo" parameterType="demo">
		INSERT INTO DEV VALUES (SEQ_DEV_NO.NEXTVAL,#{devName},#{devAge},#{devEmail},#{devGender}, #{devLang, typeHandler=strArr} ) <!-- config가서 mapper만들어줌 -->
			
			<!-- <foreach collection="devLang" item="l" open="'" separator="," close="'">
				${l}
			</foreach>) -->
	</insert>
	
	<select id="selectDemo" resultMap="demoMap"> SELECT * FROM DEV </select>
	
	<resultMap type="demo" id="demoMap">
		<result column="devlang" property="devLang" typeHandler="strArr"/>
	</resultMap>
</mapper>
